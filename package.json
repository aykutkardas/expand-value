{
  "name": "expand-value",
  "version": "1.0.0",
  "description": "Expand deeply nested values from an object, with support for advanced features.",
  "repository": "jonschlinkert/expand-value",
  "bugs": {
    "url": "https://github.com/jonschlinkert/expand-value/issues"
  },
  "homepage": "https://github.com/jonschlinkert/expand-value",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "funding": {
    "url": "https://github.com/sponsors/jonschlinkert"
  },
  "files": [
    "index.js",
    "lib"
  ],
  "license": "MIT",
  "scripts": {
    "test": "mocha",
    "cover": "nyc --reporter=text --reporter=html mocha"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "is-number": "^7.0.0"
  },
  "devDependencies": {
    "gulp-format-md": "^2.0.0",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0"
  },
  "keywords": [
    "accessor",
    "dot-prop",
    "expand-value",
    "get",
    "get-value",
    "key",
    "nested",
    "object",
    "path",
    "paths",
    "prop",
    "properties",
    "property",
    "props",
    "resolve-value",
    "segment",
    "set-value",
    "value",
    "values"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "related": {
      "list": []
    },
    "reflinks": [
      "dot-prop",
      "get-value"
    ]
  }
}